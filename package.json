{
  "name": "Ping-Test",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "prettier --write --parser typescript \"src/**/*.ts?(x)\""
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js?(x)": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "bulma": "^0.9.1",
    "vue": "^3.0.0-rc.1"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@vue/compiler-sfc": "^3.0.0-rc.1",
    "autoprefixer": "9",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "sass": "^1.26.11",
    "typescript": "^4.0.3",
    "vite": "^1.0.0-rc.4"
  }
}
